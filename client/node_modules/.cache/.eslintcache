s)\n  axios.post(url + '/api/tastings', options)\n    .then(function (response) {\n      console.log(response);\n    })\n    .catch(function (error) {\n      console.log(error);\n\n    });\n}\n\n//DELETE TASTING BY ID\nexport function deleteTasting(id: number | undefined) {\n  axios.delete(url + '/api/tastings/' + `${id}`)\n    .then(function (response) {\n      console.log(response);\n    })\n    .catch(function (error) {\n      console.log(error);\n    });\n}\n\n// CREATE NEW USER\nexport function postUser(options: { mail: string, password: string }) {\n  axios.post(url + '/api/user', options)\n    .then(function (response) {\n      console.log(response);\n    })\n    .catch(function (error) {\n      console.log(error);\n    });\n}\n\nexport interface User {\n  id: number,\n  mail: string,\n  password: string,\n}\n\n// GET ALL USERS\nexport async function getUsers(): Promise<User[]> {\n  try {\n    const response = await axios.get(url + '/api/allusers');\n    // console.log(response);\n    return response.data;\n  } catch (error) {\n    console.error(error);\n    throw error\n  }\n\n}\n","/home/carla/virtual-wine-tasting-app/client/src/Components/wineProfile/BodyRating.tsx",["92"],"import Rating from '@material-ui/lab/Rating';\nimport Typography from '@material-ui/core/Typography';\nimport Box from '@material-ui/core/Box'\nimport FiberManualRecordIcon from '@material-ui/icons/FiberManualRecord';\nimport { withStyles } from '@material-ui/core/styles';\nimport { useState, ChangeEvent } from 'react';\nimport bodyPicture from '../pictures/body.svg'\n\nconst StyledRating = withStyles({\n  iconFilled: {\n    color: 'black',\n  },\n  iconHover: {\n    color: 'black',\n  },\n})(Rating);\n\ninterface Props {\n  body: number,\n  updateBody: (newValue: number) => void\n}\n\nfunction BodyRating({ body, updateBody }: Props) {\n\n  const [statusImage, setStatusImage] = useState(true)\n\n  function hideOrDisplayImage() {\n    if (statusImage === true) {\n      setStatusImage(false)\n    } else setStatusImage(true)\n  }\n\n  return (<div>\n    <div className={\"centered__container__profile\"}>\n      <h2 className='rating__box__headline'>rate the body of the wine</h2>\n      <div className='star__rating'>\n        <Box component=\"fieldset\" mb={3} borderColor=\"transparent\" className='rating__box__profile'>\n          <Typography component=\"legend\"></Typography>\n          <StyledRating\n            className=\"style__rating\"\n            name=\"body\"\n            precision={0.5}\n            value={body}\n            color='black'\n            icon={<FiberManualRecordIcon fontSize=\"inherit\" />}\n            onChange={(event, newValue) => newValue ? updateBody(newValue) : null}\n          />\n        </Box>\n        <div className='profile__tasting__description__text'>\n\n          <details onClick={() => hideOrDisplayImage()}>\n            <summary>How to rate the body</summary>\n            <p> Body is not a scientific term, but a categorization of stlye from lightes to boldest. The four characteristics of sweetnes, acidity, tannin, and alcohol each affect how light or bold a wine will taste. You can use terms like \"light-bodied\" or \"full-bodied\" to describe the style of wine you want to drink. Lighter wines e.g. have more acidity, lower alcohol, less tannin, less sweetness.</p>\n          </details>\n        </div>\n      </div>\n      <img src={bodyPicture} alt=\"lemon\" className={statusImage === true ? '' : 'hidden__image'}></img>\n    </div>\n  </div>\n  )\n}\n\nexport default BodyRating\n\n\n","/home/carla/virtual-wine-tasting-app/client/src/Components/wineProfile/FruitRating.tsx",[],"/home/carla/virtual-wine-tasting-app/client/src/Components/wineProfile/TanninsRating.tsx",["93"],"import Rating from '@material-ui/lab/Rating';\nimport Typography from '@material-ui/core/Typography';\nimport Box from '@material-ui/core/Box'\nimport FiberManualRecordIcon from '@material-ui/icons/FiberManualRecord';\nimport { withStyles } from '@material-ui/core/styles';\nimport tannin from '../pictures/tannin.svg';\nimport { useState, ChangeEvent } from \"react\";\n\nconst StyledRating = withStyles({\n  iconFilled: {\n    color: 'black',\n  },\n  iconHover: {\n    color: 'black',\n  },\n})(Rating);\n\n\ninterface Props {\n  tannins: number,\n  updateTannins: (newValue: number) => void\n}\n\nfunction TanninsRating({ tannins, updateTannins }: Props) {\n\n  const [statusImage, setStatusImage] = useState(true);\n\n  function hideOrDisplayImage() {\n    if (statusImage === true) {\n      setStatusImage(false)\n    } else setStatusImage(true)\n  }\n\n  return (<div>\n    <div className={\"centered__container__profile\"}>\n\n      <h2 className='rating__box__headline'>rate tannins in the wine</h2>\n      <div className='star__rating'>\n        <Box component=\"fieldset\" mb={3} borderColor=\"transparent\" className='rating__box__profile'>\n          <Typography component=\"legend\"></Typography>\n          <StyledRating\n            className=\"style__rating\"\n            name=\"tannins\"\n            precision={0.5}\n            value={tannins}\n            icon={<FiberManualRecordIcon fontSize=\"inherit\" />}\n            onChange={(event, newValue) => newValue ? updateTannins(newValue) : null}\n          />\n        </Box>\n        <div className='profile__tasting__description__text'>\n\n          <details onClick={() => hideOrDisplayImage()}>\n            <summary>How to rate tannins in wine</summary>\n            <p>To taste tannin in wine, focus on the texture on your toungue. A high tannin wine will remove proteins from your tongue, causing a drying and puckering sensation. This sensation is often described as 'grippy'. High tannin wines act as palate cleanser to rich, fatty meats, cheeses and pasta dishes.</p>\n          </details>\n        </div>\n      </div>\n      <img src={tannin} alt=\"lemon\" className={statusImage === true ? '' : 'hidden__image'}></img>\n    </div>\n  </div>)\n}\n\nexport default TanninsRating\n\n","/home/carla/virtual-wine-tasting-app/client/src/Components/wineProfile/AcidityRating.tsx",["94"],"import Rating from '@material-ui/lab/Rating';\nimport Typography from '@material-ui/core/Typography';\nimport Box from '@material-ui/core/Box'\nimport FiberManualRecordIcon from '@material-ui/icons/FiberManualRecord';\nimport { withStyles } from '@material-ui/core/styles';\nimport { useState, ChangeEvent } from 'react';\nimport lemon from '../pictures/lemon.svg'\n\nconst StyledRating = withStyles({\n  iconFilled: {\n    color: 'black',\n  },\n  iconHover: {\n    color: 'black',\n  },\n})(Rating);\n\ninterface Props {\n  acidity: number\n  updateAcidity: (newValue: number) => void\n}\n\nfunction AcidityRating({ acidity, updateAcidity }: Props) {\n\n  const [statusImage, setStatusImage] = useState(true)\n\n  function hideOrDisplayImage() {\n    if (statusImage === true) {\n      setStatusImage(false)\n    } else setStatusImage(true)\n  }\n\n  return (<div>\n    <div className={\"centered__container__profile\"}>\n      <h2 className='rating__box__headline'>rate the acidity of the wine</h2>\n      <div className='star__rating'>\n        <Box component=\"fieldset\" mb={3} borderColor=\"transparent\" className='rating__box__profile'>\n          <Typography component=\"legend\"></Typography>\n          <StyledRating\n            className=\"style__rating\"\n            name=\"acidity\"\n            precision={0.5}\n            value={acidity}\n            icon={<FiberManualRecordIcon fontSize=\"inherit\" />}\n            onChange={(event, newValue) => newValue ? updateAcidity(newValue) : null}\n          />\n        </Box>\n        <div className='profile__tasting__description__text'>\n          <details onClick={() => hideOrDisplayImage()}>\n            <summary>How to rate acidity of wine</summary>\n            <p>Acids are the primary attribute that contributes to wine's tart and sour flavor. Most acids from grapes including tartaric, malic and citric acid. Like many fruits, wine lies on the acid side of pH scale, ranging from about 2.5-4.5 pH (7 is neutral). One useful thing to know about acidity in wine is that, as grapes ripen, they become less acidic. Thus, a wine from a cooler climate where it is hard to ripen grapes will produce wines with higher acidity.</p>\n          </details>\n        </div>\n      </div>\n      <img src={lemon} alt=\"lemon\" className={statusImage === true ? '' : 'hidden__image'}></img>\n    </div>\n  </div>)\n}\n\nexport default AcidityRating\n","/home/carla/virtual-wine-tasting-app/client/src/Components/wineFlavors/DominantFlavors.tsx",["95"],"import { useState } from \"react\";\nimport WineDB, { Flavor, Wine } from '../wineDB/WineDB'\n\ninterface Props {\n  updateDominantFlavors: (args: any) => void,\n  grape: string,\n  wineDB: { [key: string]: Wine }\n}\n\n\nexport default function DominantFlavors({\n  updateDominantFlavors,\n  grape,\n  wineDB,\n}: Props) {\n\n  // Partial<Type>\n  // Constructs a type with all properties of Type set to optional. This utility will return a type that represents all subsets of a given type.\n\n  // Record<Keys,Type>\n  // Constructs an object type whose property keys are Keys and whose property values are Type. This utility can be used to map the properties of a type to another type.\n  const [flavors, setFlavors] = useState<Partial<Record<Flavor, Flavor>>>({});\n\n  function updateFlavors(flavor: Flavor) {\n    if (flavors[flavor]) {\n      delete flavors[flavor];\n      setFlavors((prev) => ({\n        ...prev,\n      }));\n    } else {\n      setFlavors((prev) => ({\n        ...prev,\n        [flavor]: flavor,\n      }));\n    }\n  }\n\n  return (<div>\n\n    <div className={\"centered__container__dominant__flavors\"}>\n      <span className=\"dominant__flavor__headline\">\n        <h2>dominant flavors in {grape}</h2>\n      </span>\n      <div className='container__and__button'>\n        <div className=\"dominant__flavors__container\">\n          {wineDB[grape].dominantFlavors.map((flavor) => (\n            <div key={flavor} onClick={() => updateFlavors(flavor)} className={(flavors[flavor] === flavor ? 'dominant__flavors__box__toggled' : 'dominant__flavors__box')}>{flavor}</div>\n\n          ))}\n\n        </div>\n        <button\n          className='dominant__flavor__btn'\n          onClick={() =>\n            updateDominantFlavors({ ratingCompleted: true, flavors: flavors })\n          }\n        >\n          next\n        </button>\n      </div>\n    </div>\n  </div>\n  );\n}\n","/home/carla/virtual-wine-tasting-app/client/src/Components/wineFlavors/PossibleFlavors.tsx",[],"/home/carla/virtual-wine-tasting-app/client/src/Components/wineOverallRating/OverallRating.tsx",[],"/home/carla/virtual-wine-tasting-app/client/src/Components/WineCard/WineCard.tsx",["96"],"/home/carla/virtual-wine-tasting-app/client/src/Components/wineDB/WineDB.ts",[],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":19,"column":6,"nodeType":"103","endLine":19,"endColumn":8,"suggestions":"104"},{"ruleId":"101","severity":1,"message":"105","line":29,"column":6,"nodeType":"103","endLine":29,"endColumn":12,"suggestions":"106"},{"ruleId":"107","severity":1,"message":"108","line":44,"column":22,"nodeType":"109","messageId":"110","endLine":44,"endColumn":24},{"ruleId":"107","severity":1,"message":"111","line":49,"column":9,"nodeType":"112","messageId":"113","endLine":49,"endColumn":15},{"ruleId":"114","severity":1,"message":"115","line":8,"column":61,"nodeType":"116","messageId":"117","endLine":8,"endColumn":62},{"ruleId":"114","severity":1,"message":"115","line":47,"column":39,"nodeType":"116","messageId":"117","endLine":47,"endColumn":40},{"ruleId":"118","severity":1,"message":"119","line":6,"column":20,"nodeType":"120","messageId":"121","endLine":6,"endColumn":31},{"ruleId":"118","severity":1